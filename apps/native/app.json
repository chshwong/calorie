{
  "expo": {
    "name": "AvoVibe",
    "slug": "avovibe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brand/app-icon/Logo_MascotOnly1024x1024.png",
    "scheme": "avovibe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/brand/app-icon/Logo_MascotOnly1024x1024.png"
    },
    "android": {
      "label": "AvoVibe",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/brand/app-icon/Logo_MascotOnly1024x1024.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.chshwong.avovibe.dev",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "avovibe",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "07d93073-40a2-48d0-8838-61cd2f1493e5"
      }
    }
  }
}
